{
  "name": "LizShawCom",
  "description": "JS Building Blocks-Based Project and Theme for LizShaw.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "@hubspot/cli": "^4.0.1",
    "@hubspot/prettier-plugin-hubl": "latest",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "prettier": "^2.7.1",
    "yarpm": "^1.2.0"
  },
  "scripts": {
    "start": "cd LizShawCom-project/LizShawCom-app && yarpm start",
    "postinstall": "cd LizShawCom-project/LizShawCom-app && yarpm install",
    "lint:js": "eslint . --ext .js,.jsx",
    "prettier": "prettier . --check",
    "watch:hubl": "hs watch LizShawCom-theme LizShawCom-theme",
    "upload:hubl": "hs upload LizShawCom-theme LizShawCom-theme",
    "deploy": "hs project upload LizShawCom-project"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}
